{
    "name": "svilborg/guzzle-file-snapshot",
    "description": "Guzzle File Snapshot Mock",
    "keywords": ["guzzle", "http", "snapshot", "unit tests", "tests", "api", "api-client", "filesystem"],
    "license": "MIT",
    "type": "project",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "GuzzleHttpMock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GuzzleHttpMock\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}